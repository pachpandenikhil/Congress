<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.webtech.homework.congress">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.webtech.homework.congress.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.webtech.homework.congress.activities.LegislatorDetailsActivity" />
        <activity android:name="com.webtech.homework.congress.activities.BillDetailsActivity" />
        <activity android:name="com.webtech.homework.congress.activities.CommitteeDetailsActivity" />
        <activity android:name="com.webtech.homework.congress.activities.AboutMeActivity"></activity>
    </application>

</manifest>